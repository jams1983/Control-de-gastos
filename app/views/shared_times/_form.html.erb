<%= form_for(@shared_time) do |f| %>
  <% if @shared_time.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@shared_time.errors.count, "error") %> prohibited this service from being saved:</h2>

      <ul>
      <% @shared_time.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  <div class="field">
    <%= f.label :month %><br />
    <%= f.select :month,  options_for_select([["","0"],["January","1"],["February","2"],["March","3"],["April","4"],["May","5"],["June","6"],["July","7"],["August","8"],["September","9"],["October","10"],["November","11"],["December","12"]],@shared_time.month) %>
  </div>
  <div class="field">
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </div>
  <div class="field">
    <%= f.label :shared_time_user_tokens, "users shared time" %><br />
	    <%= f.text_field :shared_time_user_tokens , "data-pre" => @array_users.to_json %>
		

  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>